<div class="container">
  <div class="row">
    <div class="col">
      <table class="table table-borderless">
        <thead>
          <tr>
            <th>ユーザー名</th>
          </tr>
        </thead>
        <% users.each do |user| %>
          <tbody>
            <tr>
              <td>
                <%= link_to user, class: "user_#{user.id}" do %>
                  <%= user.account_name %>
                <% end %>
              </td>
            </tr>
          </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>