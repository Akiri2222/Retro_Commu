<%= form_with model: [:admin, @user], local: true do |f| %>
  <table>
    <tr>
      <td>氏名</td>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <td>アカウント名</td>
      <td><%= f.text_field :acoount_name %></td>
    </tr>
    <tr>
      <td>メールアドレス</td>
      <td><%= f.text_field :email %></td>
    </tr>
    <tr>
      <td>自己紹介</td>
      <td><%= f.text_field :biography %></td>
    </tr>
    <tr>
      <td><%= f.submit"保存" %></td>
    </tr>
  </table>
<% end %>